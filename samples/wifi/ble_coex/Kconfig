source "Kconfig.zephyr"

menu "Nordic Coex sample"

config INTERVAL_MIN
	int "BLE min connection interval. Each unit 1.25milliseconds"
	default 80

config INTERVAL_MAX
	int "BLE max connection interval. Each unit 1.25milliseconds"
	default 80

config CONN_LATENCY
	int "BLE connection latency in milliseconds"
	default 0

config SUPERVISION_TIMEOUT
	int "BLE supervision timeout in milliseconds"
	default 1000

endmenu
